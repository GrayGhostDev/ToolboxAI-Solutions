{
  "name": "ToolboxAI-Roblox",
  "globIgnorePaths": [
    "**/package-lock.json",
    "**/node_modules",
    "**/*.test.lua",
    "**/*.spec.lua"
  ],
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$properties": {
        "LoadStringEnabled": false
      },
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "src/client"
        }
      },
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts"
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$path": "src/shared"
      },
      "RemoteEvents": {
        "$className": "Folder"
      },
      "RemoteFunctions": {
        "$className": "Folder"
      },
      "Modules": {
        "$path": "src/shared"
      },
      "SDK": {
        "$className": "Folder",
        "ToolBoxAI": {
          "$path": "src/Modules"
        }
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "Config": {
        "$path": "Config"
      },
      "Templates": {
        "$className": "Folder"
      },
      "Assets": {
        "$className": "Folder",
        "NPCTemplate": {
          "$className": "Model"
        },
        "NameTag": {
          "$className": "BillboardGui"
        }
      },
      "UI": {
        "$className": "Folder",
        "HUD": {
          "$className": "ScreenGui"
        },
        "QuizUI": {
          "$className": "ScreenGui"
        },
        "ProgressTracker": {
          "$className": "ScreenGui"
        },
        "Notifications": {
          "$className": "ScreenGui"
        }
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true,
        "StreamingEnabled": true,
        "StreamingMinRadius": 64,
        "StreamingTargetRadius": 256,
        "SignalBehavior": "Deferred"
      },
      "GeneratedContent": {
        "$className": "Folder"
      },
      "Checkpoints": {
        "$className": "Folder"
      },
      "InteractiveElements": {
        "$className": "Folder"
      },
      "TerrainFeatures": {
        "$className": "Folder"
      },
      "EducationalAssets": {
        "$className": "Folder"
      },
      "NPCs": {
        "$className": "Folder"
      },
      "QuizZones": {
        "$className": "Folder"
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Technology": "Future",
        "GlobalShadows": true,
        "ShadowSoftness": 0.1,
        "Brightness": 1.5,
        "OutdoorAmbient": [140, 140, 140],
        "Ambient": [100, 100, 100],
        "ClockTime": 14
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "$properties": {
        "ShowDevelopmentGui": true,
        "ScreenOrientation": "LandscapeLeft"
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true,
        "VolumetricAudio": "Enabled"
      }
    },

    "Chat": {
      "$className": "Chat",
      "$properties": {
        "LoadDefaultChat": true
      }
    },

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    }
  },
  "servePlaceIds": [
    8505376973
  ]
}