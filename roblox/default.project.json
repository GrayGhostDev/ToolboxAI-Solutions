{
  "name": "ToolboxAI-Roblox-Environment",
  "tree": {
    "$className": "DataModel",
    
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Scripts": {
        "$path": "Scripts/ServerScripts"
      },
      "Modules": {
        "$path": "Scripts/ModuleScripts"
      }
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "Scripts/ClientScripts"
      }
    },
    
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "RemoteEvents": {
        "$path": "RemoteEvents"
      },
      "RemoteFunctions": {
        "$path": "RemoteFunctions"
      },
      "Modules": {
        "$className": "Folder",
        "NetworkManager": {
          "$path": "Scripts/ModuleScripts/NetworkManager.lua"
        },
        "ValidationModule": {
          "$path": "Scripts/ModuleScripts/ValidationModule.lua"
        }
      }
    },
    
    "ServerStorage": {
      "$className": "ServerStorage",
      "API": {
        "$className": "Folder",
        "Dashboard": {
          "$path": "API/Dashboard"
        },
        "GhostBackend": {
          "$path": "API/GhostBackend"
        }
      },
      "Config": {
        "$path": "Config"
      },
      "Templates": {
        "$path": "Templates"
      }
    },
    
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      },
      "GeneratedContent": {
        "$className": "Folder"
      }
    }
  }
}