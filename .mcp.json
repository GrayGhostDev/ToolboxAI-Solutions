{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions"
      ],
      "disabled": [
        "**/node_modules/**",
        "**/.git/**",
        "**/venv/**",
        "**/__pycache__/**",
        "**/dist/**",
        "**/build/**",
        "**/coverage/**",
        "**/data/**"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github",
        "GrayGhostDev/ToolBoxAI-Solutions"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://eduplatform:eduplatform2024@localhost:5432/educational_platform_dev"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "docker": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-docker"
      ]
    },
    "toolboxai-mcp": {
      "command": "python",
      "args": [
        "-m",
        "core.mcp.server"
      ],
      "env": {
        "MCP_HOST": "localhost",
        "MCP_PORT": "9877",
        "PYTHONPATH": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions",
        "DATABASE_URL": "postgresql://eduplatform:eduplatform2024@localhost:5432/educational_platform_dev",
        "REDIS_URL": "redis://localhost:55007",
        "JWT_SECRET_KEY": "${JWT_SECRET_KEY}"
      },
      "cwd": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions"
    },
    "toolboxai-docker-mcp": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "toolboxai-mcp",
        "python",
        "-m",
        "core.mcp.server"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    },
    "toolboxai-docker-coordinator": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "toolboxai-agent-coordinator",
        "python",
        "-m",
        "core.mcp.servers.agent_coordinator"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    },
    "toolboxai-orchestrator": {
      "command": "python",
      "args": [
        "-m",
        "core.mcp.servers.orchestrator_server"
      ],
      "env": {
        "PYTHONPATH": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions",
        "REDIS_URL": "redis://localhost:55007"
      },
      "cwd": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions"
    },
    "toolboxai-monitor": {
