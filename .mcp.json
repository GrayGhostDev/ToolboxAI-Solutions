{
  "mcpServers": {
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=jlesbkscprldariqcbvt&features=storage%2Cbranching%2Cfunctions%2Cdevelopment%2Cdebugging%2Cdatabase%2Caccount%2Cdocs"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github",
        "GrayGhostDev/ToolBoxAI-Solutions"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${DATABASE_URL}"
      ],
      "env": {
        "DATABASE_URL": "${DATABASE_URL}"
      }
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "docker": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-docker"
      ]
    },
    "toolboxai-mcp": {
      "command": "python",
      "args": [
        "-m",
        "core.mcp.server"
      ],
      "env": {
        "MCP_HOST": "localhost",
        "MCP_PORT": "9877",
        "PYTHONPATH": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions",
        "DATABASE_URL": "${DATABASE_URL}",
        "REDIS_URL": "${REDIS_URL}",
        "JWT_SECRET_KEY": "${JWT_SECRET_KEY}"
      },
      "cwd": "/Volumes/G-DRIVE ArmorATD/Development/Clients/ToolBoxAI-Solutions"
    },
    "toolboxai-docker-mcp": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "toolboxai-mcp",
        "python",
        "-m",
        "core.mcp.server"
      ],
      "env": {
        "DOCKER_HOST": "unix:///var/run/docker.sock"
      }
    },
    "toolboxai-docker-coordinator": {
