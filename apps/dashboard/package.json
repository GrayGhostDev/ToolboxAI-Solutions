{
  "name": "toolboxai-dashboard",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.x",
    "npm": ">=10"
  },
  "overrides": {
    "axios": "^1.7.9",
    "happy-dom": "^20.0.2",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": {
      "prismjs": "^1.30.0"
    },
    "@react-three/drei": {
      "@react-three/fiber": "$@react-three/fiber",
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "glob": "^11.0.0",
    "inflight": "^2.0.0",
    "@babel/plugin-proposal-class-properties": "npm:@babel/plugin-transform-class-properties@^7.25.9",
    "@babel/plugin-proposal-object-rest-spread": "npm:@babel/plugin-transform-object-rest-spread@^7.25.9",
    "node-domexception": "^1.0.0",
    "esbuild": "^0.25.0"
  },
  "scripts": {
    "install:external": "npm install --no-bin-links",
    "install:clean": "rm -rf node_modules package-lock.json && npm install --no-bin-links",
    "dev": "vite",
    "build": "vite build",
    "build:validate": "node scripts/build.js --validate-only",
    "build:production": "node scripts/build.js --production",
    "build:analyze": "vite build --mode analyze && vite-bundle-visualizer",
    "build:benchmark": "node src/scripts/performance-benchmark.js",
    "analyze:bundle": "node src/scripts/bundle-analyzer.js",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss,json}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "COVERAGE=true vitest run --coverage",
    "test:coverage:watch": "COVERAGE=true vitest --coverage",
    "test:coverage:ui": "COVERAGE=true vitest --ui --coverage",
    "test:coverage:report": "COVERAGE=true vitest run --coverage && open coverage/index.html",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf dist coverage test-reports node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "ci": "npm run validate && npm run build:validate && npm run build",
    "perf:benchmark": "npm run build && npm run build:benchmark",
    "perf:analyze": "npm run build && npm run analyze:bundle",
    "codegen": "graphql-codegen --config ../../graphql.config.yml",
    "codegen:watch": "graphql-codegen --config ../../graphql.config.yml --watch",
    "graphql:introspect": "apollo schema:download --endpoint=http://localhost:8008/graphql schema.json"
  },
  "dependencies": {
    "@apollo/client": "^3.11.0",
    "@assistant-ui/react": "^0.5.0",
    "@babel/runtime": "^7.28.4",
    "@clerk/clerk-react": "^5.47.0",
    "@emotion/is-prop-valid": "^1.4.0",
    "@mantine/charts": "^8.3.3",
    "@mantine/core": "^8.3.6",
    "@mantine/dates": "^8.3.3",
    "@mantine/form": "^8.3.2",
    "@mantine/hooks": "^8.3.6",
    "@mantine/notifications": "^8.3.6",
    "@mantine/spotlight": "^8.3.3",
    "@mantine/tiptap": "^8.3.3",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@sentry/react": "^10.22.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tabler/icons-react": "^3.35.0",
    "@tiptap/extension-link": "^3.5.1",
    "@tiptap/react": "^3.5.1",
    "@tiptap/starter-kit": "^3.5.1",
    "@types/react-window": "^2.0.0",
    "axios": "^1.7.9",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.18",
    "esbuild": "0.25.10",
    "framer-motion": "^11.18.2",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "highlight.js": "^11.11.1",
    "i18next": "^23.11.5",
    "lowlight": "^3.3.0",
    "prismjs": "^1.30.0",
    "pusher-js": "^8.4.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^14.1.2",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^15.5.1",
    "react-window": "^2.1.0",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "three": "^0.160.1",
    "tinyexec": "^0.3.2",
    "web-vitals": "^3.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@playwright/test": "^1.55.0",
    "@sentry/cli": "^2.33.0",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.14.9",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^5.6.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.2",
    "jsdom": "^24.1.3",
    "msw": "^2.11.2",
    "playwright": "^1.55.0",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "storybook": "^8.6.14",
    "terser": "^5.44.1",
    "typescript": "^5.5.4",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
