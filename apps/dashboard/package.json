{
  "name": "toolboxai-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20 <23",
    "npm": ">=9"
  },
  "overrides": {
    "axios": "^1.7.9",
    "esbuild": "0.24.2",
    "prismjs": "^1.30.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:validate": "node scripts/build.js --validate-only",
    "build:production": "node scripts/build.js --production",
    "build:analyze": "vite build --mode analyze && vite-bundle-visualizer",
    "build:benchmark": "node src/scripts/performance-benchmark.js",
    "analyze:bundle": "node src/scripts/bundle-analyzer.js",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss,json}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "COVERAGE=true vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf dist coverage test-reports node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "ci": "npm run validate && npm run build:validate && npm run build",
    "perf:benchmark": "npm run build && npm run build:benchmark",
    "perf:analyze": "npm run build && npm run analyze:bundle",
    "codegen": "graphql-codegen --config ../../graphql.config.yml",
    "codegen:watch": "graphql-codegen --config ../../graphql.config.yml --watch",
    "graphql:introspect": "apollo schema:download --endpoint=http://localhost:8008/graphql schema.json"
  },
  "dependencies": {
    "@apollo/client": "^3.11.0",
    "@assistant-ui/react": "^0.5.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.15.20",
    "@mui/x-date-pickers": "^6.20.2",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/react-window": "^1.8.8",
    "axios": "^1.7.9",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.18.2",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "i18next": "^23.11.5",
    "pusher-js": "^8.4.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-chat-elements": "^12.0.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-window": "^2.1.0",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^2.15.4",
    "three": "^0.160.1",
    "web-vitals": "^3.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@chromatic-com/storybook": "^4.1.1",
    "@emotion/cache": "^11.14.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.6.1",
    "chalk": "^5.6.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^9.1.6",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^24.1.3",
    "msw": "^2.11.2",
    "playwright": "^1.55.0",
    "storybook": "^8.6.14",
    "terser": "^5.44.0",
    "typescript": "^5.5.4",
    "vite": "^7.1.5",
    "vitest": "^1.6.0"
  }
}
