# Dashboard Environment Configuration Template
# Copy to .env.local and fill in actual values

# API Configuration
VITE_API_URL=http://127.0.0.1:8009
VITE_API_VERSION=v1

# Pusher Configuration (for real-time features)
VITE_PUSHER_KEY=your-pusher-key
VITE_PUSHER_CLUSTER=your-cluster
VITE_PUSHER_AUTH_ENDPOINT=/api/v1/pusher/auth

# Feature Flags
VITE_ENABLE_PUSHER=true
VITE_ENABLE_CLERK_AUTH=false
VITE_ENABLE_DEBUG=false

# Analytics (Optional)
VITE_GA_MEASUREMENT_ID=your-ga-id
VITE_SENTRY_DSN=your-sentry-dsn

# Environment
VITE_ENV=development
VITE_APP_NAME=ToolBoxAI Dashboard
VITE_APP_VERSION=1.0.0